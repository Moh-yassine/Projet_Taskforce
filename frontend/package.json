{
  "name": "taskforce-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "NODE_ENV=production ESLINT_USE_FLAT_CONFIG=false vite build",
    "build:analyze": "NODE_ENV=production vite build && npx vite-bundle-analyzer dist/stats.html --port 8889",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "type-check:build": "vue-tsc --noEmit --project tsconfig.build.json",
    "type-check:simple": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:build": "npm run build:prod",
    "serve": "vite preview --port 4173 --host"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.9.0",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^27.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
